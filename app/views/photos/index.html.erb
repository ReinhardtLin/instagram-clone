<table border="1px">
  <% @photos.each do |photo| %>
  <tr>
    <td><%= link_to image_tag( photo.logo.url(:thumb) ), photo.logo.url %></td>
    <td><%= photo.title %></td>
    <td><%= link_to 'Show Photo', photo_path(photo) %></td>
    <td><%= link_to 'Edit Photo', edit_photo_path(photo) %></td>
    <td><%= link_to 'Delete Photo', photo_path(photo), :method => :delete %></td>
  </tr>
  <% end %>
</table>

<%= link_to 'New Photo', new_photo_path %>